{
  "productName": "Annotator",
  "identifier": "com.snijderlab.annotator",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../frontend"
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null,
      "capabilities": [
        {
          "identifier": "main-capabilities",
          "windows": [
            "main"
          ],
          "permissions": [
            "core:path:default",
            "core:event:default",
            "core:window:default",
            "core:app:default",
            "core:resources:default",
            "core:menu:default",
            "core:tray:default",
            "core:window:allow-set-title",
            "core:webview:default",
            "dialog:allow-open",
            "dialog:allow-save"
          ]
        }
      ],
      "assetProtocol": {
        "enable": true,
        "scope": {
          "allow": [
            "../frontend"
          ],
          "deny": [
            ""
          ]
        }
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "alwaysOnTop": false,
        "title": "Annotator",
        "width": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [
      "../frontend/assets/*"
    ],
    "shortDescription": "",
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}